// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() 
{
    int a,b,c;
    cout << "Enter any three number:";
    cin >>a >>b >>c;
    
    if(a>b && a>c)
    {
        cout <<a <<" is greater";
    }
    else
    {
        if(b>c)
        {
            cout <<b <<" is greater";
        }
        else
        {
            cout <<c <<" is greater";
        }
    }

    return 0;
}
