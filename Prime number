#include <iostream>
using namespace std;
int main() 
{
    int num,count,i;
    cout << "Enter any number:";
    cin >>num;
    
    count=0;
    for(i=1;i<=num;i++)
    {
        if(num%i == 0)
        {
            count++;
        }
    }
    if(count==1 || count==2)
    {
        cout <<"Number is prime";
    }
    else
    {
        
        cout <<"Number is not prime";
    }
   
    return 0;
}
