#include <iostream>
using namespace std;

inline int mult(int x, int y)
{
    int c;
    c=x*y;
    return c;
}
inline int divi(int a, int b)
{
    int d;
    d=a/b;
    return d;
}

int main()
{
    int m,n,o,p;
    m=40;
    n=2;
    o=mult(m,n);
    
    cout <<"Multiplication is:"<<o;
    
    p=divi(m,n);
    cout <<"Division is:"<<p;
    return 0;
}
